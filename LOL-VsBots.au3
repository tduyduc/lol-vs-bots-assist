	GUICtrlSetData($Combo2, $combotext, $actions[$completeaction])
	GUICtrlSetTip($Combo2, "Chọn hành động mà bạn muốn thực hiện khi hoàn thành đủ số ván theo giới hạn.", _
			"Hành động khi hoàn thành")
	Global $Label9 = GUICtrlCreateLabel("Đẩy đường", 8, 248, 125, 25)
	$combotext = ""
	For $combopart In $lanes
		$combotext &= "|" & $combopart
	Next
	Global $Combo3 = GUICtrlCreateCombo("", 144, 248, 125, 25, 3)
	GUICtrlSetData($Combo3, $combotext, $lanes[$clearinglane])
	GUICtrlSetTip($Combo3, "Chọn một đường mà bạn muốn tự động dọn lính." & @CRLF & _
			"Chỉ hoạt động khi đạt được đầy đủ các điều kiện sau (trừ chế độ đi theo đồng đội): " & _
			@CRLF & @TAB & "(1) sử dụng màu tương ứng đội trong cài đặt hình ảnh; " & _
			@CRLF & @TAB & "(2) hiển thị vật phẩm trong cửa hàng ở dạng ô lớn (để mua đồ khởi đầu)." & @CRLF & _
			"Chú ý rằng chức năng này hoạt động an toàn nhất ở chế độ đi theo đồng đội mặc dù chưa ổn định." & @CRLF & _
			"Nếu không sử dụng chức năng này, bạn sẽ di chuyển ngẫu nhiên thiếu kiểm soát nhưng có thể chết ít hơn.", _
			"Tự động đẩy đường")
	Global $Label10 = GUICtrlCreateLabel("Mua vật phẩm", 8, 278, 125, 25)
	Global $Input4 = GUICtrlCreateInput($item, 144, 278, 125, 25)
	GUICtrlSetTip($Input4, "Nhập tên vật phẩm mà bạn sẽ mua với bảng mã Unicode, ví dụ: Ma Vũ Song Kiếm." & @CRLF & _
			"Chú ý rằng auto sẽ không mua các vật phẩm thành phần của nó mà chỉ mua vật phẩm hoàn chỉnh." & @CRLF & _
			"Nên mua vật phẩm thành phần nâng chỉ số nhiều nhất có thể mà không cần quá nhiều vàng để mua, "  & @CRLF & _
			"ví dụ: BF (Kiếm B.F.), Song Kiếm, Gậy Quá Khổ." & @CRLF & _
			"Có thể bỏ trống để không mua vật phẩm nào." & @CRLF & _
			"Chỉ hoạt động nếu các phím nóng đối với cửa hàng chưa bị sửa đổi.", _
			"Mua vật phẩm")
	Global $Button1 = GUICtrlCreateButton("Hướng dẫn", 8, $GUIHeight - 35, 125, 25)
	GUICtrlSetTip($Button1, "Đọc thêm các thông tin quan trọng về auto.", "Hướng dẫn")
	Global $Button2 = GUICtrlCreateButton("Chạy auto", 144, $GUIHeight - 35, 125, 25, 1)
	GUICtrlSetTip($Button2, "Mở LOL trước khi chạy auto." & @CRLF & "Dùng tổ hợp phím Shift+Pause để tạm dừng.", _
			"Bắt đầu tự động hoá")
	MovingOnlyCheck()
	GUISetState(@SW_SHOW)
	Do
		Switch GUIGetMsg()
			Case $Checkbox1 ; khi có sự thay đổi về chế độ chỉ di chuyển
				MovingOnlyCheck()
			Case -3 ; thoát
				SaveSettings()
				Exit
			Case $Button1 ; hướng dẫn
				MsgBox(64, "Hướng dẫn", $about & @CRLF & @CRLF & _
						"* Sử dụng auto này để tự động di chuyển lung tung, tránh thông báo AFK trong chế độ đấu với máy." & @CRLF & _
						"* Bạn cũng có thể sử dụng chế độ chỉ di chuyển để AFK trong bất cứ chế độ chơi nào." & @CRLF & _
						"* Bạn có thể di chuột vào một ô nhập dữ liệu trong giao diện thiết đặt để đọc chi tiết về thiết đặt tương ứng." & @CRLF & _
						"* Thiết đặt của bạn sẽ được lưu tự động khi bạn chạy auto hoặc thoát cửa sổ thiết đặt." & @CRLF & _
						"* Bạn có thể dùng tổ hợp phím Shift+Pause để tạm dừng hoặc dừng hẳn auto bất cứ lúc nào. " & _
						"Tuy nhiên, nếu đang ở trong game, bạn sẽ phải dùng tổ hợp phím Alt+Tab để chuyển ra một cửa sổ khác trước." & @CRLF & _
						"* Auto chỉ di chuyển trong game khi cửa sổ game được kích hoạt." & @CRLF & _
						"* Tắt gõ tiếng Việt để các phím kỹ năng hoạt động đúng." & @CRLF & _
						"* Trước khi chạy auto, hãy đảm bảo rằng bạn đang ở sảnh chờ và đang không tìm trận. " & _
						"Độ phân giải của client sẽ được thay đổi tự động thành 1024×576. " & _
						"Bạn có thể thay đổi trước thành độ phân giải này để bỏ qua quá trình thay đổi." & @CRLF & _
						"* Auto chỉ hoạt động đúng khi bạn đặt tỷ lệ hiển thị bản đồ nhỏ là 100 và các phím nóng sau được đặt theo mặc định: " & _
						"Dùng nhanh phép bất kể cài đặt (Shift + Phím kỹ năng); Phép bổ trợ (D & F); Nhấp di chuyển (Chuột phải); " & _
						"Nhấp di chuyển tấn công (Shift + Chuột phải); Mở cửa hàng (P); Tìm kiếm vật phẩm trong cửa hàng (Ctrl + Enter); " & _
						"Bật tắt bảng điểm (O); Chọn đồng minh (F1–F5).")
			Case $Button2 ; chạy auto
				SaveSettings()
				ExitLoop
		EndSwitch
	Until 0
#EndRegion

#Region Main Automation
	HotKeySet("+{pause}", "Terminate") ; phím nóng tạm dừng
	If $movingonly Then
		TraySetToolTip("LOL Vs. Bots Assist — T.D. Stoneheart" & @CRLF & "Đang sử dụng chế độ chỉ di chuyển.")
		InGameMove()
	Else
		WinActivate($title)
		$size = WinGetClientSize($title)
		If Not IsArray($size) Then Exit MsgBox(16, "Lỗi", "Chưa mở LOL.") ; 16: error flag
		While Not ($size[0] = 1024 And $size[1] = 576)
			WinMove($title, "", 0, 0, Default, Default)
			Do ; cài đặt
				ControlClick($title, "", "", "main", 1, Round(979 / 1024 * $size[0]), Round(16 / 576 * $size[1])) ; nút cài đặt
				Sleep(100)
			Until WinActive($title) And PixelGetColor(Round(234 / 1024 * $size[0]), Round(155 / 576 * $size[1]), WinGetHandle($title)) = 0xC89B3C
			Do ; chọn độ phân giải
				ControlClick($title, "", "", "main", 1, Round(476 / 1024 * $size[0]), Round(232 / 576 * $size[1])) ; đổi độ phân giải
				ControlClick($title, "", "", "main", 1, Round(476 / 1024 * $size[0]), Round(262 / 576 * $size[1])) ; tuỳ chọn 1: 1024×576
				$size = WinGetClientSize($title)
				If Not IsArray($size) Then Exit MsgBox(16, "Lỗi", "Chưa mở LOL.")
				Sleep(200)
			Until $size[0] = 1024 And $size[1] = 576
			WinActivate($title)
			ControlClick($title, "", "", "main", 1, 511, 508) ; xong cài đặt
		WEnd
		WinActivate($title)
		ControlClick($title, "", "", "main", 2, 99, 32) ; đấu / sảnh
		Sleep(500)
		WinActivate($title)
		Sleep(500)
		ControlClick($title, "", "", "main", 1, 114, 75) ; đấu với máy
		ControlClick($title, "", "", "main", 1, 347, 173) ; Summoner's Rift
		ControlClick($title, "", "", "main", 1, 347, $intermediate ? 429 : 400) ; trung bình / dễ
		While Not InChampSelect() ; màn hình chọn tướng?
			ControlClick($title, "", "", "main", 1, 820, 60) ; loại bỏ pop-up
			Sleep(300)
			ControlClick($title, "", "", "main", 1, 99, 32) ; đấu / sảnh
			Sleep(300)
			ControlClick($title, "", "", "main", 1, 396, 547) ; xác nhận, tạo phòng, tìm trận
			Sleep(300)
			ControlClick($title, "", "", "main", 2, 509, 460) ; chấp nhận trận đấu
			Sleep(300)
		WEnd
		Do
			If $limit > 0 Then
				$combotext = ""
				Switch $completeaction
					Case 1 ; thoát game
						$combotext = " trước khi thoát game"
					Case 2 ; ngủ
						$combotext = " trước khi ngủ"
					Case 3 ; ngủ đông
						$combotext = " trước khi ngủ đông"
					Case 4 ; tắt máy
						$combotext = " trước khi tắt máy"
				EndSwitch
				TraySetToolTip("LOL Vs. Bots Assist — T.D. Stoneheart" & @CRLF & "Đã chơi " & $matches & "/" & $limit & " ván." & @CRLF & _
						"Còn lại " & ($limit - $matches) & " ván" & $combotext & ".")
			Else
				TraySetToolTip("LOL Vs. Bots Assist — T.D. Stoneheart" & @CRLF & "Đã chơi " & $matches & " ván.")
			EndIf
			While Not WinExists($title2)
				ControlClick($title, "", "", "main", 2, 99, 32) ; đấu / sảnh
				Sleep(1000)
				While Not InChampSelect()
					ControlClick($title, "", "", "main", 2, 99, 32) ; đấu / sảnh
					Sleep(500)
					ControlClick($title, "", "", "main", 2, 509, 452) ; chấp nhận trận đấu
					Sleep(500)
					ControlClick($title, "", "", "main", 2, 509, 480) ; nút chấp nhận / khoá tướng
					Sleep(500)
					ControlClick($title, "", "", "main", 1, 512, 316) ; gặp lỗi khi khoá tướng?
					Sleep(1000)
					If WinExists($title2) Then ExitLoop 2
				WEnd
				ControlClick($title, "", "", "main", 2, 805, 39) ; loại bỏ thông báo che mất thanh tìm kiếm
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 705, 118) ; 6
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 635, 118) ; 5
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 545, 118) ; 4
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 465, 118) ; 3
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 385, 118) ; 2
				Sleep(10)
				ControlClick($title, "", "", "main", 1, 305, 118) ; chọn tướng vị trí 1, đề phòng, dự là ngẫu nhiên
				Sleep(10)
				ControlClick($title, "", "", "main", 2, 725, 87) ; xoá tìm kiếm
				Sleep(10)
				ControlClick($title, "", "", "main", 2, 624, 87) ; tìm kiếm
				Sleep(10)
				ControlSend($title, "", "", $champ, 1) ; nhập tên tướng
				Sleep(500)
				ControlClick($title, "", "", "main", 1, 305, 118) ; chọn tướng đầu tiên tìm thấy
				Sleep(100)
				ControlClick($title, "", "", "main", 1, 305, 118) ; chọn lại cho chắc ăn
				Sleep(1000)
				ControlClick($title, "", "", "main", 1, 305, 118)
				Sleep(4000) ; cho 5 giây suy nghĩ trước khi khoá
				Do
					Sleep(1000)
					ControlClick($title, "", "", "main", 2, 509, 452) ; nút chấp nhận / khoá tướng
					Sleep(200)
					ControlClick($title, "", "", "main", 2, 509, 480) ; nút chấp nhận / khoá tướng
					Sleep(200)
					ControlClick($title, "", "", "main", 1, 512, 316) ; gặp lỗi khi khoá tướng?
					GameBreaking()
				Until WinExists($title2) Or InChampSelect()
			WEnd ; chờ cửa sổ game xuất hiện
			Do
				GameBreaking()
				InGameMove()
				WinActivate($title)
				Do
					GameBreaking()
					Sleep(100)
					ControlClick($title, "", "", "main", 1, 430, 286) ; kết nối lại
					Sleep(100)
					ControlClick($title, "", "", "main", 1, 512, 315) ; thông báo LeaverBuster, gặp lỗi khi khoá tướng
					Sleep(100)
					ControlClick($title, "", "", "main", 1, 512, 543) ; nút OK khi nhận mảnh chìa khoá, lên cấp, thông thạo
					Sleep(100)
					If Not (($limit > 0) And ($matches >= $limit - 1)) Then ; xử lý đối với ván không phải cuối cùng theo giới hạn
						ControlClick($title, "", "", "main", 1, 509, 543) ; nhận hộp tướng khi lên cấp
						Sleep(100)
						ControlClick($title, "", "", "main", 1, 559, 626) ; nút "chơi lại"
						Sleep(100)
						ControlClick($title, "", "", "main", 1, 396, 547) ; xác nhận, tạo phòng, tìm trận
						Sleep(100)
						ControlClick($title, "", "", "main", 2, 509, 452) ; chấp nhận trận đấu
						Sleep(100)
						ControlClick($title, "", "", "main", 2, 509, 480) ; nút khoá tướng
					EndIf
					If InChampSelect() Then ExitLoop 2
					Sleep(100)
					If WinExists($title2) Then ContinueLoop 2 ; quay về InGameMove() khi kết nối lại
				Until 0
			Until 0
			$matches += 1
		Until ($limit > 0) And ($matches >= $limit)
		ControlClick($title, "", "", "main", 1, 1012, 75) ; huỷ tìm trận
		HotKeySet("+{pause}") ; tạm dừng không còn tác dụng
		$combotext = "Đã hoàn thành " & $limit & " ván!" & @CRLF
		Switch $completeaction
			Case 1 ; thoát game ; 305 = 256 (default button 2) + 48 (warning) + 1 (OK/cancel) ; xử lý OK (1) và hết giờ (-1)
				If MsgBox(305, "Hoàn thành", $combotext & "Bạn có thể huỷ bỏ quá trình thoát game trong vòng 15 giây.", 15) < 2 Then CloseLeague()
			Case 2 ; ngủ
				CloseLeague()
				If MsgBox(305, "Hoàn thành", $combotext & "Bạn có thể huỷ bỏ quá trình ngủ trong vòng 15 giây.", 15) < 2 Then Shutdown(32)
			Case 3 ; ngủ đông
				CloseLeague()
				If MsgBox(305, "Hoàn thành", $combotext & "Bạn có thể huỷ bỏ quá trình ngủ đông trong vòng 15 giây.", 15) < 2 Then Shutdown(64)
			Case 4 ; tắt máy
				CloseLeague()
				If MsgBox(305, "Hoàn thành", $combotext & "Bạn có thể huỷ bỏ quá trình tắt máy trong vòng 15 giây.", 15) < 2 Then Shutdown(5)
			Case Else
				MsgBox(64, "Hoàn thành", $combotext) ; 64: information flag
		EndSwitch
	EndIf
#EndRegion

Func Terminate() ; tạm dừng hoặc thoát
	HotKeySet("+{pause}")
	If MsgBox(36, "Tạm dừng", "Bạn có muốn thoát auto bây giờ không?") = 6 Then Exit ; 36 = 32 (warning) + 4 (yes/no)
	If Not WinActivate($title2) Then WinActivate($title)
	HotKeySet("+{pause}", "Terminate")
EndFunc

Func InChampSelect() ; kiểm tra màn hình chọn tướng
	Return PixelGetColor(259, 31, WinGetHandle($title)) = 0x57431B And PixelGetColor(765, 31, WinGetHandle($title)) = 0x57431B
EndFunc

Func DifficultyFromGUI()
	For $i = 0 To UBound($difficulties) - 1
		If GUICtrlRead($Combo1) = $difficulties[$i] Then Return $i
	Next
	Return 1
EndFunc

Func CompleteActionFromGUI()
	For $i = 0 To UBound($actions) - 1
		If GUICtrlRead($Combo2) = $actions[$i] Then Return $i
	Next
	Return 0
EndFunc

Func ClearingLaneFromGUI()
	For $i = 0 To UBound($lanes) - 1
		If GUICtrlRead($Combo3) = $lanes[$i] Then Return $i
	Next
	Return 0
EndFunc

Func MovingOnlyCheck() ; vô hiệu hoá một số thiết đặt khi bật chế độ chỉ di chuyển
	Local $i = (GUICtrlRead($Checkbox1) = 1) ? 128 : 64
	GUICtrlSetState($Input1, $i)
	GUICtrlSetState($Input3, $i)
	GUICtrlSetState($Combo1, $i)
	GUICtrlSetState($Combo2, $i)
EndFunc

Func CloseLeague() ; thoát game
	ProcessClose("LeagueClient.exe")
	ProcessClose("LeagueClientUx.exe")
	ProcessClose("LeagueClientUxRender.exe")
EndFunc

Func SaveSettings() ; lưu thiết đặt khi thoát hoặc chạy auto
	$movingonly = (GUICtrlRead($Checkbox1) = 1)
	$champ = GUICtrlRead($Input1)
	$ability = GUICtrlRead($Input2)
	$maponleft = (GUICtrlRead($Checkbox2) = 1)
	$usespells = (GUICtrlRead($Checkbox3) = 1)
	$intermediate = (DifficultyFromGUI() <> 0)
	$limit = Number(GUICtrlRead($Input3))
	$completeaction = CompleteActionFromGUI()
	$clearinglane = ClearingLaneFromGUI()
	$item = GUICtrlRead($Input4)
	$file = FileOpen("LOL-VsBots.txt", 130)
	If $file <> -1 Then
		FileWriteLine($file, Number($movingonly))
		FileWriteLine($file, $champ)
		FileWriteLine($file, $ability)
		FileWriteLine($file, Number($maponleft))
		FileWriteLine($file, Number($usespells))
		FileWriteLine($file, Number($intermediate))
		FileWriteLine($file, $limit)
		FileWriteLine($file, $completeaction)
		FileWriteLine($file, $clearinglane)
		FileWriteLine($file, $item)
		FileClose($file)
	EndIf
	GUIDelete()
EndFunc

Func GameBreaking() ; giải thoát một số lỗi trong game
	ControlClick("[TITLE:League of Legends; CLASS:#32770", _
			"You are trying to open the League of Legends game client in an invalid way.", "Button1", "main")
	ControlClick("[TITLE:Cảnh báo kết nối; CLASS:#32770]", "Bạn đã ngắt kết nối.", "Button1", "main")
	ControlClick("[TITLE:Network Warning; CLASS:#32770]", "You have disconnected.", "Button1", "main")
	ControlClick("[TITLE:Kết nối thất bại; CLASS:#32770]", "Không thể kết nối đến máy chủ.", "Button2", "main")
	ControlClick("[TITLE:Failed to Connect; CLASS:#32770]", "Unable to connect to the server.", "Button2", "main")
	ControlClick("[TITLE:Error; CLASS:#32770]", "irectX", "Button1", "main") ; DirectX
	ControlClick("[TITLE:Connection failed; CLASS:#32770]", "", "Button2", "main")
	ControlClick("[TITLE:Woops! Something broke.; CLASS:#32770]", "A problem has caused your program to close.", _
			"Button2", "main") ; BugSplat, Don't Send
EndFunc

Func BuyItem() ; mua vật phẩm theo tên
	If Not $item Then Return
	WinActivate($title2)
	$size = WinGetClientSize($title2)
	If Not IsArray($size) Then Return
	Sleep(500)
	SendKeepActive($title2)
	Send("{lshift up}{lctrl up}{rshift up}{rctrl up}")
	Send("{p down}")
	Sleep(200)
	Send("{p up}")
	Sleep(500)
	Send("^{enter}")
	Sleep(200)
	Send($item, 1)
	Sleep(500)
	Send("{enter}")
	Sleep(200)
	Send("{enter}")
	Sleep(200)
	Send("{enter}")
	Send("{lshift up}{lctrl up}{rshift up}{rctrl up}")
	Sleep(500)
	Send("{p down}")
	Sleep(200)
	Send("{p up}")
	Sleep(500)
EndFunc

Func GuessSide() ; đoán đội dựa theo bản đồ nhỏ
	WinActivate($title2)
	$size = WinGetClientSize($title2)
	If Not IsArray($size) Then Return ""
	Local $check = PixelChecksum(Round($size[0] * .95), 0, $size[0], Round($size[1] * .05), 1, WinGetHandle($title2))
	Sleep(1200)
	WinActivate($title2)
	If $check = PixelChecksum(Round($size[0] * .95), 0, $size[0], Round($size[1] * .05), 1, WinGetHandle($title2)) Then Return ""
	If $maponleft Then
		PixelSearch($size[1] / 4 * 3 / 4, $size[1] * 3 / 4, $size[1] / 4, $size[1] * 7 / 8, 0xA02B2B, 2, 1, WinGetHandle($title2))
	Else
		PixelSearch($size[0] - ($size[1] / 4) + ($size[1] / 4 * 3 / 4), $size[1] * 3 / 4, $size[0], $size[1] * 7 / 8 , _
				0xA02B2B, 2, 1, WinGetHandle($title2))
	EndIf
	If Not @error Then Return "blue"
	If $maponleft Then
		PixelSearch(0, $size[1] * 7 / 8, $size[1] / 4 / 4, $size[1], 0xA02B2B, 2, 1, WinGetHandle($title2))
	Else
		PixelSearch($size[0] - ($size[1] / 4), $size[1] * 7 / 8, $size[0] - ($size[1] / 4) + ($size[1] / 4 / 4), $size[1], _
				0xA02B2B, 2, 1, WinGetHandle($title2))
	EndIf
	Return (@error ? "" : "red")
EndFunc

Func InGameMove() ; di chuyển trong game
	Local $ticks = 0, $champindex = 1
	WinActivate($title2)
	Switch $clearinglane
		Case 1 To 3
			Do
				$side = GuessSide()
				$size = WinGetClientSize($title)
				If Not WinExists($title2) And Not BitAND(WinGetState($title), 16) And IsArray($size) Then
					WinActivate($title)
					ControlClick($title, "", "", "main", 1, Round(99 / 1024 * $size[0]), Round(32 / 576 * $size[1])) ; đấu / sảnh
					Sleep(500)
					ControlClick($title, "", "", "main", 1, Round(430 / 1024 * $size[0]), Round(286 / 576 * $size[1])) ; kết nối lại
					; WinWait($title2)
				EndIf
				Sleep(1000)
			Until $side
			$size = WinGetClientSize($title2)
			If Not $movingonly Then ; chờ lính nếu không ở chế độ chỉ di chuyển
				Sleep(10000)
				BuyItem()
				Sleep(5000)
				For $i = 1 To 6
					WinActivate($title2)
					MouseClick("menu", Random($size[0] / 5 * 2, $size[0] / 5 * 3, 1), Random($size[1] / 5 * 2, $size[1] / 5 * 3, 1), 1, 0)
					Sleep(10000)
				Next
			EndIf
	EndSwitch
	While $movingonly Or WinExists($title2)
		$size = WinGetClientSize($title2)
		If $clearinglane = 4 And Not $ticks Then $champindex = Random(2, 5, 1)
		If IsArray($size) And WinActive($title2) Then
			If Not SendKeepActive($title2) And Not $movingonly Then ExitLoop
			MouseClick("main", Round($size[0] / 2), Round($size[1] * 0.595238), 1, 0) ; chiến thắng / thất bại
			If Not Mod($ticks, 12) Then
				Send("{o 2}")
				BuyItem()
			EndIf
			Sleep(100)
			Do
				If (Not IsArray($size)) Or (Not WinExists($title2)) Then ExitLoop
				Send("{f" & $champindex & " down}")
				If $clearinglane <> 4 Or Random(0, 2, 1) Then Send("{lshift down}") ; 33% xác suất di chuyển mà không tấn công khi đi theo đồng đội
				Switch $clearinglane
					Case 1 ; top
						If $maponleft Then
							If $side = "red" Then
								PixelSearch(0, Round($size[1] * 3 / 4), Round($size[1] / 4 / 20), $size[1], _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							Else
								PixelSearch(0, Round($size[1] * 3 / 4), Round($size[1] / 4), Round(($size[1] * 3 / 4) + ($size[1] / 4 / 11)), _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							EndIf
							If @error Then
								MouseClick("menu", Round($size[1] / 4 / 7), Round(($size[1] * 3 / 4) + ($size[1] / 4 / 7)), 1, 0)
							Else
								If $side = "red" Then
									MouseClick("menu", Round($size[1] / 4 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 * 9 / 10)), 1, 0)
								Else
									MouseClick("menu", Round($size[1] / 4 * 9 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 / 10)), 1, 0)
								EndIf
							EndIf
						Else
							If $side = "red" Then
								PixelSearch(Round($size[0] - ($size[1] / 4)), Round($size[1] * 3 / 4), _
										Round($size[0] - ($size[1] / 4) + ($size[1] / 4 / 20)), $size[1], _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							Else
								PixelSearch(Round($size[0] - ($size[1] / 4)), Round($size[1] * 3 / 4), _
										$size[0], Round(($size[1] * 3 / 4) + ($size[1] / 4 / 11)), _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							EndIf
							If @error Then
								MouseClick("menu", Round($size[0] - ($size[1] / 4) +  ($size[1] / 4 / 7)), _
										Round(($size[1] * 3 / 4) + ($size[1] / 4 / 7)), 1, 0)
							Else
								If $side = "red" Then
									MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 / 10)), _
											Round(($size[1] * 3 / 4) + ($size[1] / 4 * 11 / 12)), 1, 0)
								Else
									MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 * 9 / 10)), _
											Round(($size[1] * 3 / 4) + ($size[1] / 4 / 12)), 1, 0)
								EndIf
							EndIf
						EndIf
					Case 2 ; middle
						If $maponleft Then
							If $side = "red" Then
								MouseClick("menu", Round($size[1] / 4 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 * 9 / 10)), 1, 0)
							Else
								MouseClick("menu", Round($size[1] / 4 * 9 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 / 10)), 1, 0)
							EndIf
						Else
							If $side = "red" Then
								MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 / 10)), _
										Round(($size[1] * 3 / 4) + ($size[1] / 4 * 9 / 10)), 1, 0)
							Else
								MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 * 9 / 10)), _
										Round(($size[1] * 3 / 4) + ($size[1] / 4 / 10)), 1, 0)
							EndIf
						EndIf
					Case 3 ; bottom
						If $maponleft Then
							If $side = "red" Then
								PixelSearch(0, Round(($size[1] * 3 / 4) + ($size[1] / 4 * 8 / 9)), Round($size[1] / 4), $size[1], _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							Else
								PixelSearch(Round($size[1] / 4 * 9 / 10), Round($size[1] * 3 / 4), Round($size[1] / 4), $size[1], _
										0xFFFFFF, 0, 1, WinGetHandle($title2))
							EndIf
							If @error Then
								MouseClick("menu", Round($size[1] / 4 * 5 / 6), Round(($size[1] * 3 / 4) + ($size[1] / 4 * 5 / 6)), 1, 0)
							Else
								If $side = "red" Then
									MouseClick("menu", Round($size[1] / 4 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 * 9 / 10)), 1, 0)
								Else
									MouseClick("menu", Round($size[1] / 4 * 9 / 10), Round(($size[1] * 3 / 4) + ($size[1] / 4 / 10)), 1, 0)
								EndIf
							EndIf
						Else
							If $side = "red" Then
								PixelSearch(Round($size[0] - ($size[1] / 4)), Round($size[0] - ($size[1] * 3 / 4) + ($size[1] / 4 * 8 / 9)), _
										$size[0], $size[1], 0xFFFFFF, 0, 1, WinGetHandle($title2))
							Else
								PixelSearch(Round($size[0] - ($size[1] / 4) + ($size[1] / 4 * 10 / 11)), Round($size[1] * 3 / 4), _
										$size[0], $size[1], 0xFFFFFF, 0, 1, WinGetHandle($title2))
							EndIf
							If @error Then
								MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 * 5 / 6)), _
										Round(($size[1] * 3 / 4) + ($size[1] / 4 * 5 / 6)), 1, 0)
							Else
								If $side = "red" Then
									MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 / 10)), _
											Round(($size[1] * 3 / 4) + ($size[1] / 4 * 9 / 10)), 1, 0)
								Else
									MouseClick("menu", Round($size[0] - ($size[1] / 4) + ($size[1] / 4 * 9 / 10)), _
											Round(($size[1] * 3 / 4) + ($size[1] / 4 / 10)), 1, 0)
								EndIf
							EndIf
						EndIf
					Case 4 ; follow
						MouseClick("menu", Random($size[0] / 5 * 2, $size[0] / 5 * 3, 1), Random($size[1] / 5 * 2, $size[1] / 5 * 3, 1), 1, 0)
					Case Else ; random
						If $maponleft Then
							MouseClick("menu", Random(0, $size[1] / 4, 1), Random($size[1] * 3 / 4, $size[1]), 1, 0)
						Else
							MouseClick("menu", Random($size[0] - ($size[1] / 4), $size[0], 1), Random($size[1] * 3 / 4, $size[1]), 1, 0)
						EndIf
				EndSwitch
				Send("{f" & $champindex & " up}{lshift up}")
				MouseMove(Random($size[0] / 3, $size[0] / 3 * 2, 1), Random($size[1] / 3, $size[1] / 3 * 2, 1), 0)
				For $i = 1 To StringLen($ability)
					Send("^" & StringMid($ability, $i, 1) & "+" & StringMid($ability, $i, 1)) ; ^Q+Q (Ctrl+Q, Shift+Q)
				Next
				If $usespells Then Send("+d+f")
			Until 1
		EndIf
		If Not WinExists($title2) And Not BitAND(WinGetState($title), 16) Then
			$size = WinGetClientSize($title)
			If IsArray($size) Then
				WinActivate($title)
				ControlClick($title, "", "", "main", 1, Round(99 / 1024 * $size[0]), Round(32 / 576 * $size[1])) ; đấu / sảnh
				Sleep(500)
				ControlClick($title, "", "", "main", 1, Round(430 / 1024 * $size[0]), Round(286 / 576 * $size[1])) ; kết nối lại
				If Not $movingonly Then ExitLoop
			EndIf
		EndIf
		Sleep(1000)
		$ticks = Mod($ticks + 1, 120)
	WEnd
	SendKeepActive("")
EndFunc
