			$intermediate = True, $completeaction = 0, $combotext, $side, $clearinglane = 0, $item = ""
	Global Const $difficulties[] = ["Dễ", "Trung bình"], $actions[] = ["Không làm gì", "Thoát game", "Ngủ", "Ngủ đông", "Tắt máy"], _
