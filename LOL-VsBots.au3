#Region Read Settings
	Global $file = FileOpen("LOL-VsBots.txt", 128) ; đọc tệp lưu thiết đặt
	If $file <> -1 Then
		$movingonly = (FileReadLine($file) <> 0)
		$champ = FileReadLine($file)
		$ability = FileReadLine($file)
		$maponleft = (FileReadLine($file) <> 0)
		$usespells = (FileReadLine($file) <> 0)
		$intermediate = (FileReadLine($file) <> 0)
		$limit = Round(Number(FileReadLine($file)))
		If ($limit > 999) Or ($limit < 0) Then $limit = 0
		$completeaction = Round(Number(FileReadLine($file)))
		If ($completeaction >= UBound($actions)) Or ($completeaction < 0) Then $completeaction = 0
		$clearinglane = Round(Number(FileReadLine($file)))
		If ($clearinglane >= UBound($lanes)) Or ($clearinglane < 0) Then $clearinglane = 0
		$item = FileReadLine($file)
		FileClose($file)
	EndIf
#EndRegion

